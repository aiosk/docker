[global]
	interfaces = 192.168.1.2/28
	bind interfaces only = Yes
	security = SHARE
	map to guest = Bad User
	log file = /dev/stdout
	load printers = No

[Videos]
	path = /shares/Videos
	guest ok = Yes

[data]
	path = /shares
	valid users = andry
	force user = andry
	force group = share
	read only = No
